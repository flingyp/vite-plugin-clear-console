{
  "name": "package-name",
  "version": "0.1.1",
  "type": "module",
  "packageManager": "pnpm@7.1.7",
  "description": "Starter template for TypeScript library",
  "main": "./dist/bundle.cjs",
  "module": "./dist/bundle.mjs",
  "types": "./dist/bundle.d.ts",
  "scripts": {
    "release": "standard-version",
    "first-release": "pnpm run release -- --first-release",
    "prepare": "husky install",
    "commit": "git-cz",
    "dev": "rollup --config rollup.config.ts --watch",
    "build": "rollup --config rollup.config.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "TypeScript",
    "Template Library"
  ],
  "homepage": "https://github.com/flingyp/package-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flingyp/package-name.git"
  },
  "bugs": "https://github.com/flingyp/package-name/issues",
  "author": "FlingYP",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup": "^2.77.0",
    "rollup-plugin-dts": "^4.2.2",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
